<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".cleaning">

    <EditText
        android:id="@+id/cleaning"
        android:layout_width="409dp"
        android:layout_height="101dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="       Home Cleaning"
        android:textSize="40dp"
        android:textStyle="bold"
        android:background="@color/teal_200"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.071"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.071" />

    <Button
        android:id="@+id/arlingtonCleaningServices"
        android:layout_width="404dp"
        android:layout_height="113dp"
        android:backgroundTint="@color/teal_200"
        android:text="1. Arlington Cleaning Services"
        android:textColor="@color/black"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cleaning"
        app:layout_constraintVertical_bias="0.202" />

    <Button
        android:id="@+id/DallasCleaning"
        android:layout_width="407dp"
        android:layout_height="93dp"
        android:text="2. Dallas Cleaning Services"
        android:textSize="30dp"
        android:backgroundTint="@color/teal_200"
        android:textColor="@color/black"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.75"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/arlingtonCleaningServices"
        app:layout_constraintVertical_bias="0.291" />
</androidx.constraintlayout.widget.ConstraintLayout>